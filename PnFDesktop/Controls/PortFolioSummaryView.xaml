                                                <Setter Property="Background">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentAbove10EmaRising}" Value="True">
                                                <Setter Property="BorderBrush">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentAbove10EmaFalling}" Value="True">
                                                <Setter Property="Background">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentAbove10EmaFalling}" Value="True">
                                                <Setter Property="BorderBrush">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentAbove10EmaBuy}" Value="True">
                                                <Setter Property="Foreground">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="{Binding MarketPercentAboveEma10, Converter={StaticResource OpacityConverter}, ConverterParameter={x:Static classes:SignalOpacitySignal.Buy}}"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentAbove10EmaSell}" Value="True">
                                                <Setter Property="Foreground">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="{Binding MarketPercentAboveEma10, Converter={StaticResource OpacityConverter}, ConverterParameter={x:Static classes:SignalOpacitySignal.Sell}}"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Button.Style>
                            </Button>
                        </DataTemplate>
                    </DataGridTemplateColumn.CellTemplate>
                </DataGridTemplateColumn>
                <DataGridTemplateColumn Header="Market&#x0a;Hi-Low" SortMemberPath="MarketHighLowIndexValue">
                    <DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Button Command="{Binding DataContext.LoadMarketHighLowIndexChartCommand, RelativeSource={RelativeSource AncestorType=local:PortfolioSummaryView}}"
                                    CommandParameter="{Binding}"
                                    ToolTip="Open High-Low Index P &amp; F Chart">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="↑" Visibility="{Binding MarketNewEvents, Converter={StaticResource FlagConverter}, ConverterParameter={x:Static model:IndexEvents.HighLowGt30}}"/>
                                    <TextBlock Margin="5,0"
                                               Text="{Binding MarketHighLowIndexValue, StringFormat={}{0:F0}%, Converter={StaticResource RoundingConverter}}"/>
                                    <TextBlock Text="↓" Visibility="{Binding MarketNewEvents, Converter={StaticResource FlagConverter}, ConverterParameter={x:Static model:IndexEvents.HighLowLt70}}"/>
                                </StackPanel>
                                <Button.Style>
                                    <Style TargetType="Button" BasedOn="{StaticResource IndicatorButton}">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding  MarketHighLowIndexRising}" Value="True">
                                                <Setter Property="Background">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding  MarketHighLowIndexRising}" Value="True">
                                                <Setter Property="BorderBrush">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketHighLowIndexFalling}" Value="True">
                                                <Setter Property="Background">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketHighLowIndexFalling}" Value="True">
                                                <Setter Property="BorderBrush">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding  MarketHighLowIndexBuy}" Value="True">
                                                <Setter Property="Foreground">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="{Binding MarketHighLowIndexValue, Converter={StaticResource OpacityConverter}, ConverterParameter={x:Static classes:SignalOpacitySignal.Buy}}"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketHighLowIndexSell}" Value="True">
                                                <Setter Property="Foreground">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="{Binding MarketHighLowIndexValue, Converter={StaticResource OpacityConverter}, ConverterParameter={x:Static classes:SignalOpacitySignal.Sell}}"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Button.Style>
                            </Button>
                        </DataTemplate>
                    </DataGridTemplateColumn.CellTemplate>
                </DataGridTemplateColumn>
                <!--#endregion-->
                <!--#region Sector indicators -->
                <DataGridTemplateColumn Header="Sector&#x0a;Bullish %" SortMemberPath="SectorBullishPercent">
                    <DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Button Command="{Binding DataContext.LoadSectorBullishPercentChartCommand, RelativeSource={RelativeSource AncestorType=local:PortfolioSummaryView}}"
                                    CommandParameter="{Binding}">
                                <StackPanel Orientation="Horizontal">
