<UserControl x:Class="PnFDesktop.Controls.PortfolioSummaryView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:PnFDesktop.Controls"
             xmlns:model="clr-namespace:PnFData.Model;assembly=PnFData"
             xmlns:classes="clr-namespace:PnFDesktop.Classes"
             xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="1500"
             d:DataContext="{Binding PortfolioSummaryViewModel, Source={StaticResource Locator}}"
             Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <CollectionViewSource x:Key="cvsDays" Source="{Binding Days}"/>
        <CollectionViewSource x:Key="cvsShares" Source="{Binding Shares}"/>
        <classes:FlagToVisibilityConverter x:Key="FlagConverter"/>
        <classes:FlagToBooleanConverter x:Key="FlagBooleanConverter"/>
        <classes:DoubleRoundingConverter x:Key="RoundingConverter" />
        <classes:SignalToOpacityConverter x:Key="OpacityConverter" />
        <Style x:Key="IndicatorButton" TargetType="Button">
            <Setter Property="Width" Value="70"/>
            <Setter Property="Height" Value="20" />
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="FontWeight" Value="Heavy" />
            <Setter Property="FontSize" Value="14"/>
        </Style>
    </UserControl.Resources>
    <Grid Margin="5">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="5"/>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="5"/>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="5"/>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="5"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="5"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" Grid.Column="0" Text="Portfolio"/>
        <TextBlock Grid.Row="0" Grid.Column="2" Grid.ColumnSpan="6" Text="{Binding Portfolio.Name}"/>
        <Button Grid.Row="0" Grid.Column="6" Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}" Padding="0" HorizontalAlignment="Left"
                Command="{Binding RefreshCommand}">
            <Button.ToolTip>
                <TextBlock Text="Refresh the portfolio view." />
            </Button.ToolTip>
            <Image Source="pack://application:,,,/Resources/Images/icons8-refresh-32.png" 
                                StretchDirection="Both"  Width="18" Height="18"/>
        </Button>

        <TextBlock Grid.Row="2" Grid.Column="0" Text="Selected Date"/>
        <ComboBox Grid.Row="2" Grid.Column="2" 
                      SelectedItem="{Binding SelectedDay, UpdateSourceTrigger=PropertyChanged}" 
                      ItemsSource="{Binding Source={StaticResource cvsDays}}"
                      Width="100" HorizontalAlignment="Left">
            <ComboBox.ItemTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding DayDescription}"/>
                </DataTemplate>
            </ComboBox.ItemTemplate>
        </ComboBox>
        <TextBlock Grid.Row="2" Grid.Column="4" Text="Custom Date"/>
        <xctk:DateTimePicker  Grid.Row="2" Grid.Column="6"  
                              Value="{Binding CustomDay}"
                              Format="ShortDate"
                              Width="100"
                              ShowButtonSpinner="False"
                              TimePickerVisibility="Collapsed"
                              AutoCloseCalendar="True"/>
        <DataGrid Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="8"
            ItemsSource="{Binding Source={StaticResource cvsShares}}"
                  CanUserAddRows="False"
                  CanUserDeleteRows="False"
                  AutoGenerateColumns="False"
                  SelectionMode="Single"
                  SelectionUnit="FullRow"
                  IsReadOnly="True">
            <DataGrid.Columns>
                <DataGridTemplateColumn Header="TIDM" SortMemberPath="Tidm" IsReadOnly="True">
                    <DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <TextBlock Text="{Binding Tidm}" ToolTipService.IsEnabled="{Binding Remarks, Converter={StaticResource StringNotEmpty}}">
                                <TextBlock.ToolTip>
                                    <TextBlock Text="{Binding Remarks}" />
                                </TextBlock.ToolTip>
                            </TextBlock>
                        </DataTemplate>
                    </DataGridTemplateColumn.CellTemplate>
                </DataGridTemplateColumn> 
                <DataGridTextColumn Header="Name" Binding="{Binding Name}"  IsReadOnly="True"/>
                <DataGridTextColumn Header="Notices" Binding="{Binding Notices}"  IsReadOnly="True"/>
                <!--#region Market information -->
                <DataGridTemplateColumn Header="Market&#x0a;%RS Buy" SortMemberPath="MarketPercentRsBuy">
                    <DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Button Command="{Binding DataContext.LoadMarketPercentRSBuyChartCommand, RelativeSource={RelativeSource AncestorType=local:PortfolioSummaryView}}"
                                    CommandParameter="{Binding}"
                                    ToolTip="Open % of shares on RS buy signal P &amp; F Chart">
                                <Button.Content>
                                    <TextBlock Text="{Binding MarketPercentRsBuy, StringFormat={}{0:F0}%, Converter={StaticResource RoundingConverter}}"/>
                                </Button.Content>
                                <Button.Style>
                                    <Style TargetType="{x:Type Button}" BasedOn="{StaticResource IndicatorButton}">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding MarketPercentRsBuyRising}" Value="True">
                                                <Setter Property="Background">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentRsBuyRising}" Value="True">
                                                <Setter Property="BorderBrush">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentRsBuyFalling}" Value="True">
                                                <Setter Property="Background">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentRsBuyFalling}" Value="True">
                                                <Setter Property="BorderBrush">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentRsBuyBuy}" Value="True">
                                                <Setter Property="Foreground">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="{Binding MarketPercentRsBuy, Converter={StaticResource OpacityConverter}, ConverterParameter={x:Static classes:SignalOpacitySignal.Buy}}"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentRsBuySell}" Value="True">
                                                <Setter Property="Foreground">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="{Binding MarketPercentRsBuy, Converter={StaticResource OpacityConverter}, ConverterParameter={x:Static classes:SignalOpacitySignal.Buy}}"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Button.Style>
                            </Button>
                        </DataTemplate>
                    </DataGridTemplateColumn.CellTemplate>
                </DataGridTemplateColumn>
                <DataGridTemplateColumn Header=" Market&#x0a;%RS Xs" SortMemberPath="MarketPercentRsRising">
                    <DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Button Command="{Binding DataContext.LoadMarketPercentRSRisingChartCommand, RelativeSource={RelativeSource AncestorType=local:PortfolioSummaryView}}"
                                    CommandParameter="{Binding}"
                                    ToolTip="Open % of shares with rising RS P &amp; F Chart">
                                <Button.Content>
                                    <TextBlock Text="{Binding MarketPercentRsRising, StringFormat={}{0:F0}%, Converter={StaticResource RoundingConverter}}"/>
                                </Button.Content>
                                <Button.Style>
                                    <Style TargetType="Button" BasedOn="{StaticResource IndicatorButton}">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding MarketPercentRsRisingRising}" Value="True">
                                                <Setter Property="Background">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentRsRisingRising}" Value="True">
                                                <Setter Property="BorderBrush">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentRsRisingFalling}" Value="True">
                                                <Setter Property="Background">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentRsRisingFalling}" Value="True">
                                                <Setter Property="BorderBrush">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentRsRisingBuy}" Value="True">
                                                <Setter Property="Foreground">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="{Binding MarketPercentRsRising, Converter={StaticResource OpacityConverter}, ConverterParameter={x:Static classes:SignalOpacitySignal.Buy}}"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentRsRisingSell}" Value="True">
                                                <Setter Property="Foreground">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="{Binding MarketPercentRsRising, Converter={StaticResource OpacityConverter}, ConverterParameter={x:Static classes:SignalOpacitySignal.Sell}}"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Button.Style>
                            </Button>
                        </DataTemplate>
                    </DataGridTemplateColumn.CellTemplate>
                </DataGridTemplateColumn>
                <DataGridTemplateColumn Header="Market&#x0a;%PT" SortMemberPath="MarketPercentPositiveTrend">
                    <DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Button Command="{Binding DataContext.LoadMarketPercentPTChartCommand, RelativeSource={RelativeSource AncestorType=local:PortfolioSummaryView}}"
                                    CommandParameter="{Binding}"
                                    ToolTip="Open % of shares trading above bullish support line P &amp; F Chart">
                                <Button.Content>
                                    <TextBlock Text="{Binding MarketPercentPositiveTrend, StringFormat={}{0:F0}%, Converter={StaticResource RoundingConverter}}"/>
                                </Button.Content>
                                <Button.Style>
                                    <Style TargetType="{x:Type Button}" BasedOn="{StaticResource IndicatorButton}">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding MarketPercentPositiveTrendRising}" Value="True">
                                                <Setter Property="Background">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentPositiveTrendRising}" Value="True">
                                                <Setter Property="BorderBrush">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentPositiveTrendFalling}" Value="True">
                                                <Setter Property="Background">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentPositiveTrendFalling}" Value="True">
                                                <Setter Property="BorderBrush">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentPositiveTrendBuy}" Value="True">
                                                <Setter Property="Foreground">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="{Binding MarketPercentPositiveTrend, Converter={StaticResource OpacityConverter}, ConverterParameter={x:Static classes:SignalOpacitySignal.Buy}}"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentPositiveTrendSell}" Value="True">
                                                <Setter Property="Foreground">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="{Binding MarketPercentPositiveTrend, Converter={StaticResource OpacityConverter}, ConverterParameter={x:Static classes:SignalOpacitySignal.Sell}}"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Button.Style>
                            </Button>
                        </DataTemplate>
                    </DataGridTemplateColumn.CellTemplate>
                </DataGridTemplateColumn>
                <DataGridTemplateColumn Header="Market&#x0a;Bullish %" SortMemberPath="MarketBullishPercent">
                    <DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Button Command="{Binding DataContext.LoadMarketBullishPercentChartCommand, RelativeSource={RelativeSource AncestorType=local:PortfolioSummaryView}}"
                                    CommandParameter="{Binding}">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="↑" Visibility="{Binding MarketNewEvents, Converter={StaticResource FlagConverter}, ConverterParameter={x:Static model:IndexEvents.BullConfirmed}}"/>
                                    <TextBlock Text="↑↑" Visibility="{Binding MarketNewEvents, Converter={StaticResource FlagConverter}, ConverterParameter={x:Static model:IndexEvents.BullConfirmedLt30}}"/>
                                    <TextBlock Margin="5,0"
                                               Text="{Binding MarketBullishPercent, StringFormat={}{0:F0}%, Converter={StaticResource RoundingConverter}}"/>
                                    <TextBlock Text="↓" Visibility="{Binding MarketNewEvents, Converter={StaticResource FlagConverter}, ConverterParameter={x:Static model:IndexEvents.BearConfirmed}}"/>
                                    <TextBlock Text="↓↓" Visibility="{Binding MarketNewEvents, Converter={StaticResource FlagConverter}, ConverterParameter={x:Static model:IndexEvents.BearConfirmedGt70}}"/>
                                </StackPanel>
                                <Button.Style>
                                    <Style TargetType="Button" BasedOn="{StaticResource IndicatorButton}">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding MarketBullishPercentRising}" Value="True">
                                                <Setter Property="Background">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketBullishPercentRising}" Value="True">
                                                <Setter Property="BorderBrush">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketBullishPercentFalling}" Value="True">
                                                <Setter Property="Background">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketBullishPercentFalling}" Value="True">
                                                <Setter Property="BorderBrush">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketBullishPercentBuy}" Value="True">
                                                <Setter Property="Foreground">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="{Binding MarketBullishPercent, Converter={StaticResource OpacityConverter}, ConverterParameter={x:Static classes:SignalOpacitySignal.Buy}}"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketBullishPercentSell}" Value="True">
                                                <Setter Property="Foreground">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="{Binding MarketBullishPercent, Converter={StaticResource OpacityConverter}, ConverterParameter={x:Static classes:SignalOpacitySignal.Sell}}"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Button.Style>
                            </Button>
                        </DataTemplate>
                    </DataGridTemplateColumn.CellTemplate>
                </DataGridTemplateColumn>
                <DataGridTemplateColumn Header="Market&#x0a;%30 EMA" SortMemberPath="MarketPercentAboveEma30">
                    <DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Button Command="{Binding DataContext.LoadMarketPercentAbove30EmaChartCommand, RelativeSource={RelativeSource AncestorType=local:PortfolioSummaryView}}"
                                    CommandParameter="{Binding}"
                                    ToolTip="Open % of shares above their own 30 week EMA P &amp; F Chart">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="↑" Visibility="{Binding MarketNewEvents, Converter={StaticResource FlagConverter}, ConverterParameter={x:Static model:IndexEvents.PercentOf30Gt30}}"/>
                                    <TextBlock Margin="5,0"
                                               Text="{Binding MarketPercentAboveEma30, StringFormat={}{0:F0}%, Converter={StaticResource RoundingConverter}}"/>
                                    <TextBlock Text="↓" Visibility="{Binding MarketNewEvents, Converter={StaticResource FlagConverter}, ConverterParameter={x:Static model:IndexEvents.PercentOf30Lt70}}"/>
                                </StackPanel>
                                <Button.Style>
                                    <Style TargetType="Button" BasedOn="{StaticResource IndicatorButton}">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding MarketPercentAbove30EmaRising}" Value="True">
                                                <Setter Property="Background">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentAbove30EmaRising}" Value="True">
                                                <Setter Property="BorderBrush">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentAbove30EmaFalling}" Value="True">
                                                <Setter Property="Background">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentAbove30EmaFalling}" Value="True">
                                                <Setter Property="BorderBrush">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="0.2"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentAbove30EmaBuy}" Value="True">
                                                <Setter Property="Foreground">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Green" Opacity="{Binding MarketPercentAboveEma30, Converter={StaticResource OpacityConverter}, ConverterParameter={x:Static classes:SignalOpacitySignal.Buy}}"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MarketPercentAbove30EmaSell}" Value="True">
                                                <Setter Property="Foreground">
                                                    <Setter.Value>
                                                        <SolidColorBrush Color="Red" Opacity="{Binding MarketPercentAboveEma30, Converter={StaticResource OpacityConverter}, ConverterParameter={x:Static classes:SignalOpacitySignal.Sell}}"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Button.Style>
                            </Button>
                        </DataTemplate>
                    </DataGridTemplateColumn.CellTemplate>
                </DataGridTemplateColumn>
                <DataGridTemplateColumn Header="Market&#x0a;%10 EMA" SortMemberPath="MarketPercentAboveEma10">
                    <DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Button Command="{Binding DataContext.LoadMarketPercentAbove10EmaChartCommand, RelativeSource={RelativeSource AncestorType=local:PortfolioSummaryView}}"
                                    CommandParameter="{Binding}"
                                    ToolTip="Open % of shares above their own 10 week EMA P &amp; F Chart">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="↑" Visibility="{Binding MarketNewEvents, Converter={StaticResource FlagConverter}, ConverterParameter={x:Static model:IndexEvents.PercentOf10Gt30}}"/>
                                    <TextBlock Margin="5,0"
                                               Text="{Binding MarketPercentAboveEma10, StringFormat={}{0:F0}%, Converter={StaticResource RoundingConverter}}"/>
                                    <TextBlock Text="↓" Visibility="{Binding MarketNewEvents, Converter={StaticResource FlagConverter}, ConverterParameter={x:Static model:IndexEvents.PercentOf10Lt70}}"/>
                                </StackPanel>
                                <Button.Style>
